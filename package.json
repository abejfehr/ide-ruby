{
  "name": "ide-ruby",
  "version": "0.1.2",
  "description": "Atom-IDE support for ruby language.  Most of the functionality comes from https://github.com/kwerle/ruby_language_server",
  "main": "src/main.js",
  "scripts": {
    "lint": "eslint src/",
    "test": "npm run lint"
  },
  "repository": "https://github.com/kwerle/ide-ruby.git",
  "keywords": [
    "ruby",
    "ruby-parsing",
    "ide",
    "language-server-protocol",
    "outline-view"
  ],
  "author": {
    "name": "Kurt Werle"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kwerle/ide-ruby/issues"
  },
  "homepage": "https://github.com/kwerle/ide-ruby#readme",
  "devDependencies": {
    "eslint": "^4.7.1"
  },
  "dependencies": {
    "atom-languageclient": "^0.6.4"
  },
  "non-dependencies": {
    "ruby-language-server": "^0.0.1"
  },
  "engines": {
    "atom": ">=1.21.0"
  },
  "configSchema": {
    "dockerPath": {
      "type": "string",
      "default": "/usr/local/bin/docker",
      "description": "Absolute path to the docker command"
    },
    "imageName": {
      "type": "string",
      "default": "kwerle/ruby_language_server:latest",
      "description": "Ruby language server docker image to run (was mtsmfm/language_server)"
    }
  },
  "enhancedScopes": [
    "source.ruby"
  ],
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "find-references": {
      "versions": {
        "0.1.0": "provideFindReferences"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    }
  },
  "atomRequirements": [
    "atom-ide-ui",
    "language-ruby"
  ],
  "atomCommands": {
    "ide-ruby:help": "Provides help information and descriptions of commands."
  }
}
